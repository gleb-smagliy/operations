{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "rootDir": "./__generated",
      "outDir": "./dist",
      "allowJs": false,
      "strictNullChecks": true,
      "declaration": true
    },
    "exclude": [
      "node_modules"
    ]
  }
  